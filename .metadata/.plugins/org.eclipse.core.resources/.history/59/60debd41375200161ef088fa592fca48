
public class math {

	public static void main(String[] args) {
		int a=34;
		int b=-746;
		double c=2455.7;
		
		System.out.println("Math ceil" + a + " : " + Math.ceil(a));
		System.out.println("Math ceil" + b + " : " + Math.ceil(b));
		System.out.println("Math ceil" + c + " : " + Math.ceil(c));
		
	}

}
