import java.util.Scanner;

public class UserInput3 {

	public static void main(String[] args) {
		Scanner azizultex = new Scanner( System.in);
		
		System.out.println("Enter first value:\t");
		String firstv = azizultex.nextLine();
		
		System.out.println("Enter second value:\t");
		int secv = azizultex.nextInt();
		
		System.out.println("Your first value is: " + firstv);
		System.out.println("Your second value is: " + secv);
	}

}
